package com.android.cardsgame.entities;

import android.os.Parcel;

import java.io.Serializable;

/**
 * Created by Salman on 10/27/2015.
 */
public class User implements Serializable {

    public int id,score;
    public String name;

    public User(int id, String name , int score) {
        this.id = id;
        this.name = name;
        this.score=score;
    }

}
